<Window x:Class="get_serial_number_demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:get_serial_number_demo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <Button x:Name="btn_1" Click="btn_1_Click" Width="100" Height="66" Margin="10,50" Content="1"/>
                <Button x:Name="btn_2" Click="btn_2_Click" Width="100" Height="66" Margin="10,50" Content="2"/>
                <Button x:Name="btn_3" Click="btn_3_Click" Width="100" Height="66" Margin="10,50" Content="3"/>
                <Button x:Name="btn_4" Click="btn_4_Click" Width="100" Height="66" Margin="10,50" Content="4"/>
                <Button x:Name="btn_5" Click="btn_5_Click" Width="100" Height="66" Margin="10,50" Content="5"/>
            </StackPanel>
            <ListBox x:Name="list_number" Height="200"></ListBox>
        </StackPanel>
    </Grid>
</Window>
